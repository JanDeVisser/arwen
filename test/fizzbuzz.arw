func puts(s: string) void -> "libarwenrt:arwen$puts"
func putln(s: string) void -> "libarwenrt:arwen$putln"
func puti(i: i64) void -> "libarwenrt:arwen$puti"

func main() i32 {
    i := 1
    while i < 17 {
        if (i % 15) == 0 {
            putln("FizzBuzz")
        } else if (i % 3) == 0 {
            putln("Fizz")
        } else if (i % 5) == 0 {
            putln("Buzz")
        } else {
            puti(i)
            putln("")
        }
        i += 1
    }
}
